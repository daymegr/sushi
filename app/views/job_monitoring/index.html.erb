<h3>Job List</h3>

<table><tr><th>Job ID</th><th>Status</th><th>Log</th><th>Application</th><th>Time (Start/End)</th><th>Result</th></tr>
<% @job_list.each do |job| %>
  <tr>
    <td><%= job[0] %></td><td><%= job[1] %></td>
    <td><%= link_to 'Log', :action => 'print_log', :job_id => job[0] %></td>
    <td><%= job[2] %></td><td><%= job[3].to_s %></td>
    <td><%= link_to @results[job[0]], @results[job[0]] %></a></td>
  </tr>
<% end %>
</table>
